{
  "compilerOptions": {
    "outDir": "./dist/",  
    "sourceMap": true,                                
    "module": "es6",
    "target": "es5", 
    "jsx": "react",
    "allowJs": true,  
    "moduleResolution": "node",                             
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  }
}
